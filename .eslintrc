{
  "env": {
    "browser": true,
    "es6": true,
    "cypress/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:cypress/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "plugins": ["cypress", "@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-explicit-any": ["off"]
  }
}
